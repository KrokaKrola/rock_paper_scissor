    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "use-sound": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
